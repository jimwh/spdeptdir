<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <!-- no key, no index for loading data purpose -->
    <changeSet author="jh3389" id="table2">
        <createTable schemaName="foobar" tableName="level2">
            <column name="ID" type="INT">
                <constraints unique="true" nullable="false"/>
            </column>
            <column name="directory_name" type="VARCHAR(150)">
                <constraints nullable="false"/>
            </column>
            <column name="parent" type="INT"/>
            <column name="tie_line" type="VARCHAR(50)"/>
            <column name="address" type="VARCHAR(50)"/>
            <column name="mail_code" type="VARCHAR(50)"/>
            <column name="phone_type" type="VARCHAR(50)"/>
            <column name="phone_number" type="VARCHAR(50)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>