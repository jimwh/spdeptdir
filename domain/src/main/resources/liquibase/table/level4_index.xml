<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <!-- indexing on directory_name -->
    <changeSet author="jh3389" id="alter-level4" >
        <sql>ALTER TABLE level4 MODIFY id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY</sql>
    </changeSet>

    <changeSet author="jh3389" id="sequence-level4" >
        <sql>ALTER TABLE level4 AUTO_INCREMENT=559526</sql>
    </changeSet>

    <changeSet author="jh3389" id="directory_name_level4">
        <createIndex catalogName="deptdir"
                     indexName="directory_name_level4"
                     schemaName="deptdir"
                     tableName="level4"
                     unique="false">
            <column name="name" type="VARCHAR(150)"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>